{
	"name": "fastify",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/fastify/src",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nx/webpack:webpack",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"target": "node",
				"compiler": "tsc",
				"outputPath": "dist/packages/fastify",
				"main": "packages/fastify/src/main.ts",
				"tsConfig": "packages/fastify/tsconfig.app.json",
				"assets": ["packages/fastify/src/assets"],
				"isolatedConfig": true,
				"webpackConfig": "packages/fastify/webpack.config.js"
			},
			"configurations": {
				"development": {},
				"production": {}
			}
		},
		"serve": {
			"executor": "@nx/js:node",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "fastify:build"
			},
			"configurations": {
				"development": {
					"buildTarget": "fastify:build:development"
				},
				"production": {
					"buildTarget": "fastify:build:production"
				}
			}
		},
		"lint": {
			"executor": "@nx/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["packages/fastify/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "packages/fastify/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		}
	},
	"tags": []
}
