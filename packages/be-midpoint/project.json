{
  "name": "be-midpoint",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/be-midpoint/src",
  "projectType": "application",
  "targets": {
    "build": {
			"executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/packages/be-midpoint",
        "main": "packages/be-midpoint/src/main.ts",
        "tsConfig": "packages/be-midpoint/tsconfig.app.json",
        "assets": ["packages/be-midpoint/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "packages/be-midpoint/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
		},
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "be-midpoint:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "be-midpoint:build:development"
        },
        "production": {
          "buildTarget": "be-midpoint:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/be-midpoint/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/be-midpoint/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
